{
  "name": "miquel-points",
  "main": "./src/worker/index.ts",
  "compatibility_date": "2026-02-25",
  "compatibility_flags": ["nodejs_compat"],
  "assets": {
    "directory": "./dist/client",
    "not_found_handling": "single-page-application"
  },
  "observability": {
    "enabled": true
  },
  "vars": {
    "DEV": false
  },
  "routes": ["points.miquelpuigturon.com/*"],
  "d1_databases": [
    {
      "binding": "points_db",
      "database_name": "points_db",
      "database_id": "034188b7-2d29-446f-b42a-839c66c55294"
    }
  ],
  "ratelimits": [
    {
      "namespace_id": "1000",
      "name": "RATE_LIMITER",
      "simple": {
        "limit": 5,
        "period": 60
      }
    }
  ]
}
